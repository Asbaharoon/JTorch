Float:float:Float:CFloat:TH_REAL_IS_FLOAT:double
Double:double:Double:CDouble:TH_REAL_IS_DOUBLE:double

UByte:short:Byte:CUInt8:TH_REAL_IS_BYTE:long
Byte:byte:Char:CInt8:TH_REAL_IS_CHAR:long
Short:short:Short:CInt16:TH_REAL_IS_SHORT:long
Int:int:Int:CInt32:TH_REAL_IS_INT:long
Long:long:Long:CInt64:TH_REAL_IS_LONG:long
